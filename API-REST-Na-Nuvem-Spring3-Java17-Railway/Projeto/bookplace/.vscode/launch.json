        "projectName": "Bookplace",
        "args": "--spring.profiles.active=dev",
        "env": {
          "SPRING_PROFILES_ACTIVE": "dev"
        }
      },
      {
        "name": "Spring Boot: Prd",
        "type": "java",
        "request": "launch",
        "mainClass": "org.bookplace.Bookplace",
        "projectName": "Bookplace",
        "args": "--spring.profiles.active=prd",
        "envFile": ".env"
      }
    ]
  }
  