{
    "version": "0.0.1",
    "configurations": [
      {
        "type": "java",
        "name": "Bookplace",
        "request": "launch",
        "mainClass": "org.bookplace.Bookplace",
        "projectName": "bookplace",
        "envFile": "../.env"
      },
      {
        "name": "Spring Boot: Dev",
        "type": "java",
